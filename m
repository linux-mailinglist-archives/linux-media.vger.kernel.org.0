> +static void rkvdec_hevc_run_preamble(struct rkvdec_ctx *ctx,
> +				=C2=A0=C2=A0=C2=A0=C2=A0 struct rkvdec_hevc_run *run)
> +{
> +	struct v4l2_ctrl *ctrl;
